{
  "name": "revolut",
  "version": "1.0.0",
  "description": "revolut test",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint src --ext .js -c .eslintrc",
    "lint:fix": "eslint src --ext .js -c .eslintrc --fix",
    "start": "webpack-dev-server",
    "build": "webpack -p --config webpack.config.js",
    "server": "node ./server.js",
    "start:local": "npm run build && npm run server",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "precommit": "npm run test && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Akallabet/revolut.git"
  },
  "keywords": [
    "revolut",
    "react",
    "redux",
    "jest"
  ],
  "author": "Graziano Statello",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Akallabet/revolut/issues"
  },
  "homepage": "https://github.com/Akallabet/revolut#readme",
  "dependencies": {
    "antd": "^3.6.1",
    "axios": "^0.18.0",
    "path": "^0.12.7",
    "prop-types": "^15.6.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^22.4.4",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.6.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.1",
    "eslint": "^4.13.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "iconfont-webpack-plugin": "^2.0.0",
    "jest": "^23.1.0",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "less-vars-to-js": "^1.2.1",
    "postcss-loader": "^2.1.5",
    "prettier": "^1.13.4",
    "prettylint": "^1.0.0",
    "react-styleguidist": "^7.3.5",
    "standard": "^10.0.3",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.10.2",
    "webpack-cli": "^2.1.5",
    "webpack-dev-server": "^3.1.1"
  },
  "jest": {
    "roots": [
      "<rootDir>/__test__/",
      "<rootDir>/src/",
      "<rootDir>/__mocks__/"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|less|svg)$": "<rootDir>/__mocks__/style-mock.js"
    },
    "modulePaths": [
      "<rootDir>/src"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!antd|css-animation|rc-pagination|rc-calendar|rc-util|rc-tooltip)"
    ],
    "setupFiles": [
      "<rootDir>/jest-configs/setup-tests.js"
    ],
    "verbose": true
  }
}
